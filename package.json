{
  "name": "unikrn-native",
  "version": "1.0.0",
  "description": "Unikrn react native app",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "lint": "eslint ./src/**",
    "prettier": "prettier --config .prettierrc --check src/**",
    "check-staged": "lint-staged"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webonise/unikrn-native.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/webonise/unikrn-native/issues"
  },
  "homepage": "https://github.com/webonise/unikrn-native#readme",
  "dependencies": {
    "@expo/samples": "3.0.0",
    "@expo/vector-icons": "10.0.1",
    "axios": "0.19.0",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "expo": "33.0.0",
    "expo-asset": "5.0.0",
    "expo-constants": "5.0.0",
    "expo-font": "5.0.0",
    "expo-web-browser": "5.0.0",
    "native-base": "2.12.1",
    "prop-types": "15.7.2",
    "react": "16.8.3",
    "react-dom": "16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-calendar-strip": "1.3.8",
    "react-native-web": "0.11.4",
    "react-navigation": "3.11.0",
    "react-redux": "7.1.0",
    "redux": "4.0.1",
    "redux-promise": "0.6.0",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-install": "2.1.0",
    "babel-preset-expo": "5.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.2",
    "expo-cli": "2.20.10",
    "husky": "2.4.1",
    "jest-expo": "33.0.0",
    "lint-staged": "8.2.1",
    "prettier": "1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "prettier",
      "git add"
    ]
  }
}
